@model List<SchoolManagementSystemP1.Models.ProvinceVM>
@{
    ViewBag.Title = "ListProvince";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.ActionLink("Create New", "Create")
<!-- /.row -->
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Province Details
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="dataTable_wrapper">
                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <thead>
                            <tr>
                                <th>
                                    ProviceId
                                </th>
                                <th>
                                    ProviceName
                                </th>
                                <th>Status</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                           @foreach (var item in Model)
                           {
                            <tr>
                                <td>
                                    @item.ProvinceID
                                </td>
                                <td>
                                    @item.ProvinceName
                                </td>
                                <td>
                                    @item.Status
                                </td>
                                <td>
                                    @Html.ActionLink("Edit", "Edit", new { id = item.ProvinceID }) |
                                    @Html.ActionLink("Details", "Details", new { id = item.ProvinceID }) |
                                    @Html.ActionLink("Delete", "Delete", new { id = item.ProvinceID })
                                </td>
                            </tr>
                           }
                        </tbody>
                    </table>
                </div>
                <!-- /.table-responsive -->

            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->




